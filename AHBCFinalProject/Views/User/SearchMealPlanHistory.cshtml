@model ViewMealPlanViewModel

@{
    ViewData["Title"] = "SearchMealPlanHistory";
}

    <div class="container">
        <img src="https://live.staticflickr.com/65535/49224599768_b56ace7661_o.jpg" id="bg" alt="bg_img" style="height: 100%; width: 100%;" />
        <div class="centered">
            <div>
                <form asp-controller="User" asp-action="ViewMealPlanResults" method="post">

                    <label>Select Meals From:</label>
                    <input type="date" asp-for="StartDate" min="12-1-2019" />

                    <input type="submit" value="Search" class="btn btn-default" />
                </form>
            </div>
        </div>
    </div>
<h1>View a previous meal plan by entering a date.</h1>



